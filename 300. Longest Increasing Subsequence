class Solution(object):
    def lengthOfLIS(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if len(nums)<=1: return len(nums)
        dp = [1]*len(nums)
    
        for i in range(1, len(nums)):
            for j in range(i):
                if nums[j] < nums[i]:
                    if dp[j]+1 > dp[i]:
                        dp[i] = dp[j]+1
        return max(dp)
